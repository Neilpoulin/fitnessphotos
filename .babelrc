{
    "presets": [
        "babel-preset-expo"
    ],
    "env": {
        "development": {
            "plugins": [
                "transform-react-jsx-source"
            ]
        }
    },
    "plugins": [
        "jsx-display-if",
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ]
            }
        ]
    ]
}
